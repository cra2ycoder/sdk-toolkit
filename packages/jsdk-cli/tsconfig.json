{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "lib": ["es5", "es2015", "es2016", "es2017", "es2018", "esnext", "dom"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["./jsdk", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "dist"]
}
